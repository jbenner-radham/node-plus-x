{
  "name": "plus-x",
  "version": "0.0.0",
  "description": "Make a file executable. A portable equivalent to running the `chmod +x` command.",
  "keywords": [
    "+x",
    "chmod",
    "cli",
    "executable",
    "execute",
    "mode",
    "permission",
    "permissions",
    "terminal"
  ],
  "homepage": "https://github.com/jbenner-radham/node-plus-x#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-plus-x/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-plus-x.git"
  },
  "license": "BSD-3-Clause",
  "author": "James Benner <hello@jamesbenner.com> (https://www.jamesbenner.com/)",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    },
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "bin": {
    "plus-x": "dist/esm/cli.js"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
