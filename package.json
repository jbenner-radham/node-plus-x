  "dependencies": {
    "meow": "^14.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.0",
    "@radham/eslint-config": "^6.0.0",
    "@types/node": "^24.7.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "execa": "^9.6.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "rimraf": "^6.0.1",
    "sort-package-json": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=22"
  }
